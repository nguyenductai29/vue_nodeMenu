{"ast":null,"code":"import \"core-js/modules/es.array.includes.js\";\nimport \"core-js/modules/es.string.includes.js\";\nimport \"core-js/modules/es.regexp.exec.js\";\nimport \"core-js/modules/es.string.replace.js\";\n\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nString.prototype.bytes = function () {\n  var length = 0;\n\n  for (var i = 0; i < this.length; i++) {\n    var c = this.charCodeAt(i);\n    var isOneByte = c >= 0x0 && c < 0x81 || c === 0xf8f0 || c >= 0xff61 && c < 0xffa0 || c >= 0xf8f1 && c < 0xf8f4;\n    length += isOneByte ? 1 : 2;\n  }\n\n  return length;\n};\n\nexport default {\n  name: \"App\",\n  computed: {\n    dispHeaderIcon: function dispHeaderIcon() {\n      var menuPath = [this.$path.MENU, this.$path.MASTER_MENU, this.$path.REFERENCE_MENU];\n      return !menuPath.includes(this.$route.path);\n    },\n    subTitle: function subTitle() {\n      var subTitle = this.$store.state.title;\n      return subTitle ? \" - \".concat(subTitle) : '';\n    }\n  },\n  methods: {\n    back: function back() {\n      this.$router.back();\n    },\n    home: function home() {\n      this.$router.replace(this.$store.getters.menuPath);\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;AAYAA;AACA;;AACA;AACA;AACA;AACAC;AACA;;AACA;AACA,CARA;;AAUA;AACAC,aADA;AAEAC;AACAC,kBADA,4BACA;AACA;AACA;AACA,KAJA;AAKAC,YALA,sBAKA;AACA;AACA;AACA;AARA,GAFA;AAYAC;AACAC,QADA,kBACA;AACA;AACA,KAHA;AAIAC,QAJA,kBAIA;AACA;AACA;AANA;AAZA","names":["String","length","name","computed","dispHeaderIcon","subTitle","methods","back","home"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <v-app id=\"app\">\n    <div id=\"nav\" class=\"mb-3 font-weight-bold\">\n      <div id=\"back\" v-if=\"dispHeaderIcon\"><v-icon dark @click=\"back\">mdi-arrow-left</v-icon></div>\n      <div id=\"title\" class=\"blue-grey darken-3 white--text\">Restaurant system{{ this.subTitle }}</div>\n      <div id=\"home\" v-if=\"dispHeaderIcon\"><v-icon dark @click=\"home\">mdi-home</v-icon></div>\n    </div>\n    <router-view/>\n  </v-app>\n</template>\n\n<script>\nString.prototype.bytes = function () {\n  var length = 0\n  for (var i = 0; i < this.length; i++) {\n    var c = this.charCodeAt(i)\n    const isOneByte = (c >= 0x0 && c < 0x81) || (c === 0xf8f0) || (c >= 0xff61 && c < 0xffa0) || (c >= 0xf8f1 && c < 0xf8f4)\n    length += isOneByte ? 1 : 2\n  }\n  return length\n}\n\nexport default {\n  name: \"App\",\n  computed: {\n    dispHeaderIcon() {\n      const menuPath = [this.$path.MENU, this.$path.MASTER_MENU, this.$path.REFERENCE_MENU]\n      return !menuPath.includes(this.$route.path)\n    },\n    subTitle() {\n      const subTitle = this.$store.state.title\n      return subTitle ? ` - ${subTitle}`: ''\n    }\n  },\n  methods: {\n    back () {\n      this.$router.back()\n    },\n    home () {\n      this.$router.replace(this.$store.getters.menuPath)\n    }\n  }\n}\n</script>\n\n<style>\nhtml {\n  overflow: auto !important\n}\n\n#nav #title {\n  text-align: center;\n  padding: 15px;\n}\n\n#nav #back {\n  top: 15px;\n  left: 30px;\n  position: absolute;\n}\n\n#nav #home {\n  top: 15px;\n  right: 30px;\n  position: absolute;\n}\n\n.theme--dark.v-icon:after {\n  opacity: .24;\n}\n\n.v-text-field .v-label {\n    max-width: 100%;\n}\n\n.v-text-field .v-label--active {\n    max-width: 133%;\n}\n\n/* tabulator */\n.tabulator-cell {\n  height: 40px !important;\n  line-height: 40px !important;\n  padding: 0px 8px !important;\n}\n.tabulator-cell.tabulator-editing {\n  padding: 0px !important;\n}\n.tabulator-row.tabulator-selected {\n  background-color: #E3F2FD !important;\n}\n.tabulator-cell[tabulator-field='actions'] .v-btn {\n  opacity: .5 !important;\n  margin: 2.5px !important;\n}\n.tabulator-cell[tabulator-field='actions'] .v-icon {\n  font-size: 24px !important;\n  margin: 5px !important;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}